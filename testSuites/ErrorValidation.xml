<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Parallel Test Suite">
    <groups>
        <run>
            <include name="ErrorHandling"/>
        </run>
    </groups>
    <test thread-count="5" verbose="2" preserve-order="true" name="Submit Order">
        <classes>
            <class name="pageTests.SubmitOrderTest"/>
        </classes>
    </test>
    
    <test thread-count="5" name="Error Validation Test">
        <classes>
            <class name="pageTests.ErrorValidationsTest"/>
        </classes>
    </test>
</suite>